{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSameAccountWithMFA",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::868295555863:root"
            },
            "Action": "sts:AssumeRole",
            "Condition": {
                "Bool": {
                    "aws:MultiFactorAuthPresent": "true"
                }
            }
        }
    ]
}
